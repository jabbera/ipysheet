{
  "name": "ipysheet",
  "version": "0.4.4",
  "description": "Spreadsheet in the Jupyter notebook",
  "author": "Maarten A. Breddels",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantStack/ipysheet.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project . && webpack --mode=production",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w --project .",
    "watch:nbextension": "webpack --watch --mode=development",
    "test": "karma start --single-run"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../ipysheet/labextension/"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "chai": "^4.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "karma": "^5.0.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^5.0.2",
    "karma-typescript-es6-transform": "^5.0.2",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "typescript": "~4.1.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^4.0",
    "@jupyterlab/application": "^3.0.11",
    "handsontable": "^6.2.2",
    "lodash": "^4.17.4",
    "underscore": "^1.8.3"
  }
}
